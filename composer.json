{
    "name": "abianbiya/filehandler",
    "description": "Filehandler for storing and versioning files in Laravel",
    "keywords": [
        "abianbiya",
        "filehandler"
    ],
    "homepage": "https://github.com/abianbiya/filehandler",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Abi Anbiya",
            "email": "mail.anbiya@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3",
        "laravel/framework": "^9.0|^10.0|^11.0",
        "league/flysystem-aws-s3-v3": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "Abianbiya\\Filehandler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Abianbiya\\Filehandler\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Abianbiya\\Filehandler\\FilehandlerServiceProvider"
            ],
            "aliases": {
                "Filehandler": "Abianbiya\\Filehandler\\Facades\\Filehandler"
            }
        }
    }
}
